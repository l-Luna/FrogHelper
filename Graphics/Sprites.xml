<Sprites>
  
  <!-- The idle anim is "CollabUtils2_silverBerry" or "CollabUtils2_ghostSilverBerry", defined in Collab Utils. -->
  
  <FrogHelper_wingedSilver path="objects/FrogHelper/wingedSilver/" start="idle">
    <Center />
    
    <Loop id="idle" path="idle" delay=".08" frames="0-6,0-6,0-6,0-6,0-5,7" />
    <Anim id="collect" path="idle" delay=".07" frames="8-12" />
  </FrogHelper_wingedSilver>
  
  <FrogHelper_ghostWingedSilver path="objects/FrogHelper/wingedSilver/" start="idle">
    <Center />
    
    <Loop id="idle" path="ghost" delay=".08" frames="0-6,0-6,0-6,0-6,0-5,7" />
    <Anim id="collect" path="ghost" delay=".07" frames="8-12" />
  </FrogHelper_ghostWingedSilver>

  <FrogHelper_frogBerry path="objects/FrogHelper/frogBerry/" start="idle">
    <Center />
    
    <Loop id="idle" path="normal" delay=".08" frames="0-6,0-6,0-6,0-6,0-5,7" />
    <Anim id="collect" path="normal" delay=".07" frames="8-12" />
  </FrogHelper_frogBerry>

  <FrogHelper_ghostFrogBerry path="objects/FrogHelper/frogBerry/" start="idle">
    <Center />
    
    <Loop id="idle" path="ghost" delay=".08" frames="0-6,0-6,0-6,0-6,0-5,7" />
    <Anim id="collect" path="ghost" delay=".07" frames="8-13" />
  </FrogHelper_ghostFrogBerry>

  <FrogHelper_frogBerryShard path="objects/FrogHelper/frogBerryShard/" start="idle">
    <Center />
    
    <Loop id="idle" path="normal" delay=".08" frames="0-6,0-6,0-6,0-6,0-6" />
    <Anim id="collect" path="normal" delay=".07" frames="7-11" />
  </FrogHelper_frogBerryShard>

  <FrogHelper_ghostFrogBerryShard path="objects/FrogHelper/frogBerryShard/" start="idle">
    <Center />
    
    <Loop id="idle" path="ghost" delay=".08" frames="0-6,0-6,0-6,0-6,0-6,0-6" />
    <Anim id="collect" path="ghost" delay=".07" frames="7-11" />
  </FrogHelper_ghostFrogBerryShard>

  <FrogHelper_frogularText path="objects/FrogHelper/frogularText/" >
    <Center />

    <Anim id="fade" path="fade" delay=".07" />
  </FrogHelper_frogularText>
</Sprites>